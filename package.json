{
  "name": "alignment-sanity",
  "displayName": "Alignment Sanity",
  "description": "Virtual alignment of = : && || and/or in TypeScript, JSON, and Python without modifying files",
  "version": "2.0.0",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Formatters",
    "Other"
  ],
  "activationEvents": [
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onLanguage:json",
    "onLanguage:jsonc",
    "onLanguage:python"
  ],
  "main": "./out/extension.js",
  "author": {
    "name": "zaydek"
  },
  "publisher": "zaydek",
  "repository": "github:zaydek/alignment-sanity",
  "license": "MIT",
  "contributes": {
    "commands": [
      {
        "command": "alignment-sanity.toggle",
        "title": "Alignment Sanity: Toggle"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "postinstall": "node scripts/patch-tree-sitter.js"
  },
  "dependencies": {
    "@vscode/tree-sitter-wasm": "^0.3.0"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "20.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  }
}
